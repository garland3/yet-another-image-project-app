# Frontend development environment variables

# Enable Fast Refresh for React
FAST_REFRESH=true

# Skip preflight check to avoid version conflicts
SKIP_PREFLIGHT_CHECK=true

# Generate source maps for debugging (set to false for faster builds)
GENERATE_SOURCEMAP=true

# Disable browser opening automatically
BROWSER=none

# Development server configuration
WATCHPACK_POLLING=false

# Enable React profiling in development builds
PROFILING=false